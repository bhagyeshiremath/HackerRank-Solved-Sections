public static void main(String[] args) 
{
        Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
        double y = sc.nextDouble();
        sc.nextLine();
        String line = sc.nextLine();

        System.out.println("String: " + line);
        System.out.println("Double: " + y);
        System.out.println("Int: " +  x);
 }
